{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off"
    }
}
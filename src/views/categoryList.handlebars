<h1>List of Categories</h1>

{{#if categories}}

<div class="container">
    {{#each categories}}
    <div class="box">
        <div class="primary">{{{this.title}}}</div>
        <div class="secondary">Category with id: {{{this.id}}}</div>
        <div class="secondary">Created At: {{{this.createdAt}}}</div>
        <div class="secondary">Updated At: {{{this.updatedAt}}}</div>
    </div>
    {{/each}}
</div>

{{/if}}
